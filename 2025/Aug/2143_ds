#if 1
#include <iostream>
#include <algorithm>
using namespace std;

long long a[1000];
long long b[1000];
long long aSum[1000000]; // a의 모든 부 배열 저장
long long bSum[1000000]; // b의 모든 부 배열 저장

int main() {
	ios_base::sync_with_stdio(false);
	cin.tie(NULL);
	cout.tie(NULL);

	int T, N, M;
	cin >> T >> N;

	for (int i = 0; i < N; i++) {
		cin >> a[i];
	}

	cin >> M;
	for (int i = 0; i < M; i++) {
		cin >> b[i];
	}

	int aSize = 0;
	for (int i = 0; i < N; i++) {
		int sum = 0;
		for (int j = i; j < N; j++) {
			sum += a[j];
			aSum[aSize++] = (long long)sum;
		}
	}

	int bSize = 0;
	for (int i = 0; i < M; i++) {
		int sum = 0;
		for (int j = i; j < M; j++) {
			sum += b[j];
			bSum[bSize++] = (long long)sum;
		}
	}

    // lower_bound, upper_bound를 위한 정렬
	sort(aSum, aSum + aSize);
	sort(bSum, bSum + bSize);

	long long cnt = 0;
	for (int i = 0; i < aSize; i++) {
		long long aT = aSum[i];
		long long bT = T - aT;

		int lb = lower_bound(bSum, bSum + bSize, bT) - bSum;
		int ub = upper_bound(bSum, bSum + bSize, bT) - bSum;

		if (ub - lb) cnt += (long long)ub - lb;
	}

	cout << cnt;
}
#endif

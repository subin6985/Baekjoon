#include <iostream>
using namespace std;

struct Point {
	long long x, y;
};

long long ccw(Point A, Point B, Point C) {
	long long val = (B.x - A.x) * (C.y - A.y) - (B.y - A.y) * (C.x - A.x);
	
	if (val > 0) return 1;
	else if (val < 0) return -1;
	return 0;
}

bool isIntersect(Point A, Point B, Point C, Point D) {
	long long ab_c = ccw(A,B,C);
	long long ab_d = ccw(A,B,D);
	long long cd_a = ccw(C,D,A);
	long long cd_b = ccw(C,D,B);
	
	if (ab_c * ab_d <= 0 && cd_a * cd_b <= 0) {
		if (max(min(A.x,B.x), min(C.x,D.x)) <= min(max(A.x,B.x), max(C.x,D.x)) &&
			max(min(A.y,B.y), min(C.y,D.y)) <= min(max(A.y,B.y), max(C.y,D.y)))
			return true;
	}
	return false;
}

int main() {
	Point A, B, C, D;
	
	cin >> A.x >> A.y >> B.x >> B.y;
	cin >> C.x >> C.y >> D.x >> D.y;
	
	if (isIntersect(A,B,C,D)) cout << 1;
	else cout << 0;
}

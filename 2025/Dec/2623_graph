#include <iostream>
#include <vector>
#include <queue>
using namespace std;

// 위상 정렬
int main() {
	int N, M; cin >> N >> M;
	
	vector<vector<int>> graph(N + 1);
	vector<int> inDeg(N + 1, 0); // 진입 차수 기록
	
	for (int i = 0; i < M; i++) {
		int k; cin >> k;
		
		vector<int> v(k);
		for (int j = 0; j < k; j++) {
			cin >> v[j];
		}
		
		for (int j = 0; j < k - 1; j++) {
			graph[v[j]].push_back(v[j + 1]);
			inDeg[v[j + 1]]++;
		}
	}
	
	queue<int> q; // 진입 차수가 0인 노드 넣기
	for (int i = 1; i <= N; i++) {
		if (inDeg[i] == 0) q.push(i);
	}
	
	vector<int> res;
	
	while (!q.empty()) {
		int curr = q.front(); q.pop();
		res.push_back(curr); // 큐에 저장된 노드를 결과에 추가
		
		for (int next: graph[curr]) {
			if (--inDeg[next] == 0) // 간선 제거
				q.push(next); // 진입 차수가 0이 되면 큐에 추가
		}
	}
	
	if (res.size() != N) cout << 0; // 결과가 N개가 아니면 실패
	else {
		for (int i = 0; i < N; i++) {
			cout << res[i] << "\n";
		}
	}
}

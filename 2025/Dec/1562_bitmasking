#include <iostream>
using namespace std;

const int MOD = 1000000000;
long long dp[101][10][1 << 10]; // dp[length][lastDigit][mask]

int main() {
	int N; cin >> N;
	
	for (int i = 1; i <= 9; i++) {
		dp[1][i][1 << i] = 1;
	}
	
	for (int len = 1; len < N; len++) {
		for (int d = 0; d <= 9; d++) {
			for (int mask = 0; mask < (1 << 10); mask++) {
				if (dp[len][d][mask] == 0) continue;
				
				if (d > 0) {
					int newMask = mask | (1 << (d - 1)); // 현재보다 하나 아래 숫자일 경우 비트마스크 (계단식)
					dp[len + 1][d - 1][newMask] = (dp[len + 1][d - 1][newMask] + dp[len][d][mask]) % MOD;
				}
				if (d < 9) {
					int newMask = mask | (1 << (d + 1)); // 현재보다 하나 위 숫자일 경우 비트마스크
					dp[len + 1][d + 1][newMask] = (dp[len + 1][d + 1][newMask] + dp[len][d][mask]) % MOD;
				}
			}
		}
	}
	
	int full = (1 << 10) - 1; // 모든 숫자 사용한 비트마스크
	
	long long ans = 0;
	for (int d = 0; d <= 9; d++) {
		ans = (ans + dp[N][d][full]) % MOD;
	}
	
	cout << ans;
}
